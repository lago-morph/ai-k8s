      "Write(**)",
      "Bash(ls:*)",
      "Bash(grep:*)",
      "Bash(find:*)",
      "Bash(test:*)",
      "Bash(xargs:*)",
      "Bash(mkdir:*)",
      "Bash(touch:*)",
      "Bash(cat:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(.venv/bin/pytest:*)",
      "Bash(.venv/bin/python*:*)",
      "Bash(.venv/bin/pip:*)"
